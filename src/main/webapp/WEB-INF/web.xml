
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">

  <display-name>Servlet 3.0 Web Application</display-name>
  	<welcome-file-list>
		<welcome-file>/login.jsp</welcome-file>
	</welcome-file-list>
		
	<servlet>
		<servlet-name>CrearUsuario</servlet-name>
		<servlet-class>Usuario.CrearUsuario</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CrearUsuario</servlet-name>
		<url-pattern>/CrearUsuario</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ListarUsuario</servlet-name>
		<servlet-class>Usuario.ListarUsuario</servlet-class>
	</servlet>
		<servlet-mapping>
		<servlet-name>ListarUsuario</servlet-name>
		<url-pattern>/ListarUsuario</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>CrearRol</servlet-name>
		<servlet-class>Roles.CrearRol</servlet-class>
	</servlet>
		<servlet-mapping>
		<servlet-name>CrearRol</servlet-name>
		<url-pattern>/CrearRol</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>ListarRoles</servlet-name>
		<servlet-class>Roles.ListarRoles</servlet-class>
	</servlet>
		<servlet-mapping>
		<servlet-name>ListarRoles</servlet-name>
		<url-pattern>/ListarRoles</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>Login.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
</web-app>
